<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="739px"
    android:layout_height="689px"
    android:orientation="horizontal">

    <Button
        android:id="@+id/bigshowmanger_leftbtn"
        android:layout_width="39px"
        android:layout_height="match_parent"
        android:background="@drawable/btn_right_return" />

    <LinearLayout
        android:background="#e0ffffff"
        android:layout_width="700px"
        android:layout_height="match_parent"
        android:orientation="horizontal">
        <com.jt28.a6735.viewpager.MyViewPager
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:id="@+id/bigshowmanger_viewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

    <Button
        android:visibility="gone"
        android:id="@+id/bigshowmanger_leftbtn2"
        android:layout_width="39px"
        android:layout_height="match_parent"
        android:background="@drawable/btn_left_return"
         />

</LinearLayout>
